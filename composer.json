{
    "name": "friendsofhyperf/tinker",
    "description": "The Powerful REPL for Hyperf.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "tinker",
        "swoole"
    ],
    "authors": [{
        "name": "Huangdijia",
        "email": "huangdijia@gmail.com",
        "homepage": "https://github.com/huangdijia"
    }],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/command": "~2.2.0",
        "hyperf/contract": "~2.2.0",
        "hyperf/di": "~2.2.0",
        "hyperf/framework": "~2.2.0",
        "hyperf/utils": "~2.2.0",
        "psy/psysh": "^0.10.0|^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Tinker\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "2.x-dev": "2.0-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Tinker\\ConfigProvider"
        }
    }
}
